class Solution(object):
    def maxDepth(self, s):
        """
        :type s: str
        :rtype: int
        """
        count,maxi = 0, 0
        for i in s:
            if i == "(":
                count += 1
            elif i == ")":
                count -= 1
            maxi = max(maxi, count)
        return maxi            
